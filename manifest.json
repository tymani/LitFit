{
  "manifest_version": 2,
  "name": "LitFit",
  "version": "0.1",


  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["styles.css"],
      "js": ["jquery-2.1.1.js","content.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"]
  }
}
